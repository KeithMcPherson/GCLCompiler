{ test of relational operators}
$c+
module test2 
private
integer a,b;
 begin

 if  1 = 1 ->  write 1;
  [] 1 # 1 ->  write 0;
 fi;

 if  1 <=  1 ->  write 1;
  [] 1 >   1 ->  write 0;
 fi;

 if  1 < 2  -> write 1;
  [] 1 >= 2 -> write 0;
 fi;

 if  1 = 2 ->  write 0;
  [] 1 # 2 ->  write 1;
 fi;

 if  1 < 1  ->  write 0;
  [] 1 >= 1 ->  write 1;
 fi;

a := 1;
b := 2;
  if  a = b -> write 0;
   [] a # b -> write 1;
  fi;

 if  a < b  ->  write 1;
  [] a >= b ->  write 0;
 fi;

 if  a <= b ->  write 1;
  [] a >  b ->  write 0;
 fi;

 if  b >= a ->  write 1;
  [] b <  a ->  write 0;
 fi;

 if  a < a ->  write 0;
  [] a = a ->  write 1;
  [] a > a ->  write 0;
 fi;

 end. { all ones}
 